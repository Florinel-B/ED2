template &lt;typename T&gt;
class IteradorLevel{
using ID = typename::ID
using Type = typename T::Type
using Node = typename T::Node&lt;Type&gt;
IteradorProfundidad () : arbol{} {}
IteradorProfundidad (T elem, int nivel) : arbol(elem), level(nivel) {
auto Node = arbol.getRoot()
auto lastElem = Node
cola.push(Node)
int accLevel = 0
while(accLevel != level &amp;&amp; !cola.empty()){
auto nodeacc = cola.front()
cola.pop()
for (const auto&amp; elem: arbol.getChiledren(nodeacc)){
cola.push(elem)
}
if (lastElem == nodeacc){
lastElem = cola.back()
accLevel +=1
}}} //en el contructor llego hasta el nivel desesado teniendo en la cola al final el nivel deseado completo por lo que solo tengo que hacer pop en cada ++ y voy recorriendo el final

IteradorProfundidad&amp; operator++(){
cola.pop()
return *this
}
auto operator++(int){
auto temp = *this;
operator++();
return temp;
}
ID operator*(){
return (arbol.getElement(cola.front()))
}

bool operator!=(IteradorProfundidad other){
return !(cola.empty() &amp;&amp; other.cola.empty());
}


private : 
T arbol
std::queue&lt;ID&gt; cola;
int level;

}</textarea></div><div class="attachments"></div></div></div><div class="comment clearfix"><h4 class="accesshide">Comentarios</h4>Comentario: <p dir="ltr" style="text-align: left;">Intentas un iterador más complejo del que se pide.</p><p dir="ltr" style="text-align: left;">Una cola no tiene un .back, incumple el concepto de cola (puedes usar una bicola, pero no es el objetivo del ejercicio).</p><p dir="ltr" style="text-align: left;"><br></p></div></div></div><div class="submitbtns"></div></div></form><div class="submitbtns"><a class="mod_quiz-next-nav" href="https://www.aulavirtual.urjc.es/moodle/mod/quiz/view.php?id=10158246">Finalizar revisión</a></div></div>
                        

